{
  "name": "graphql-mongodb-typescript-blog",
  "version": "1.0.0",
  "description": "Blog starter with GraphQL,Typescript,MongoDB",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "build": "tsc -p . && ncp src/graphql/schemas/ dist/graphql/schemas",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon src/server.ts --exec ts-node src/server.ts -e ts,graphql"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "graphql",
    "apollo",
    "express"
  ],
  "author": "Universal Tutorial",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^11.15.27",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "@apollo/server": "^4.0.0",
    "@graphql-tools/schema": "^9.0.4",
    "@graphql-tools/stitch": "^8.7.23",
    "@graphql-tools/utils": "^8.12.0",
    "apollo-server-express": "^2.17.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "bull": "^4.10.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^16.6.0",
    "graphql-constraint-directive": "^4.1.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "^0.0.1",
    "graphql-tools": "^8.3.6",
    "http": "^0.0.0",
    "joi": "^17.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.10.6",
    "ncp": "^2.0.0",
    "otp-generator": "^4.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.8.4"
  }
}
